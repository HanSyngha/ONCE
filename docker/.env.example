# ONCE(main) - AWS Environment Template
# 복사: cp .env.example .env

# Domain
DOMAIN=your-ip.nip.io
PUBLIC_URL=https://your-ip.nip.io:5090
PROXY_PORT=5090

# SSL cert path (Dashboard의 certbot 공유)
CERTBOT_CONF_PATH=/home/ubuntu/dashboard/certbot/conf

# PostgreSQL
POSTGRES_USER=once
POSTGRES_PASSWORD=your_secure_password_here
POSTGRES_DB=once_web

# JWT Secret (32자 이상)
JWT_SECRET=your_jwt_secret_here_minimum_32_characters

# Developers (Super Admin)
DEVELOPERS=your_admin_id

# LLM Proxy (Dashboard API)
LLM_PROXY_URL=https://your-ip.nip.io:4090/v1/chat/completions
LLM_SERVICE_ID=once

# Frontend URL
FRONTEND_URL=https://your-ip.nip.io:5090

# OAuth
NAVER_CLIENT_ID=
NAVER_CLIENT_SECRET=
KAKAO_CLIENT_ID=
KAKAO_CLIENT_SECRET=
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
